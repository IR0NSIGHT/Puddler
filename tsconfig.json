{
    "compilerOptions": {
      "target": "es5",
      "module": "ES6",
      "outDir": "compiled",
      "noImplicitAny": true,
      "noImplicitThis": true,
      "alwaysStrict": true,
      "strictBindCallApply": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true,
      "strictPropertyInitialization": true,
      "moduleResolution": "node"
    },
    "include": [
      "src/**/*.ts"
  , "src/FileOperation"  ],
    "exclude": [
      "node_modules",
      "**/*.spec.ts",
      "**/*.test.ts",
      "src/__mocks__/*"
    ]
  }
  